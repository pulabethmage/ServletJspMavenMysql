<!DOCTYPE web-app PUBLIC
 "-//Sun Microsystems, Inc.//DTD Web Application 2.3//EN"
 "http://java.sun.com/dtd/web-app_2_3.dtd" >

<web-app>
  <display-name>Archetype Created Web Application</display-name>
  
  <servlet>
  	<servlet-name>GetBooksController</servlet-name>
  	<display-name>GetBooksController</display-name>
  	<description></description>
  	<servlet-class>co.urbanraw.web.GetBooksController</servlet-class>
  </servlet>
  
  <servlet>
  	<servlet-name>AddBooksController</servlet-name>
  	<display-name>AddBooksController</display-name>
  	<description></description>
  	<servlet-class>co.urbanraw.web.AddBooksController</servlet-class>
  </servlet> 

 <servlet-mapping>
  	<servlet-name>GetBooksController</servlet-name>
  	<url-pattern>/getBook</url-pattern>
  </servlet-mapping>
  
  <servlet-mapping>
  	<servlet-name>AddBooksController</servlet-name>
  	<url-pattern>/addBook</url-pattern>
  </servlet-mapping>
  
</web-app>
